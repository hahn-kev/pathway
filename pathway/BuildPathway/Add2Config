[guitool "B1 Clean"]
	cmd = "cd pathway; make clean clean-build"
[guitool "B2 Version"]
	cmd = git log --until=today |grep ^commit |wc |python pathway/BuildPathway/first2Ver.py
[guitool "B3 Log"]
	cmd = gedit pathway/debian/changelog
	noconsole = yes
[guitool "B4 Build"]
	cmd = "cd pathway;debuild -eBUILD_NUMBER=$ARGS -i -us -uc -b"
	argprompt = yes
