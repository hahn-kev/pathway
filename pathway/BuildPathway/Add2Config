	cmd = gedit pathway/debian/changelog
	noconsole = yes
[guitool "B4 Build"]
	cmd = "cd pathway;debuild -eBUILD_NUMBER=$ARGS -i -us -uc -b"
	argprompt = yes
[guitool "B5 Tag"]
	cmd = python pathway/BuildPathway/dateTagPkg.py
	noconsole = yes
