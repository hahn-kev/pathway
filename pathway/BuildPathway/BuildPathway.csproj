    <UpdateReadme Readme="$(ProjectDir)ReadMePw.rtf" License="$(ProjectDir)License.rtf" Template="$(ProjectDir)UserInterface-tpl.wxs" />
    <ZeroCheck />
    <FileComponents FilesTemplate="Files$(ConfTag)-tpl.wxs" FeaturesTemplate="Features$(ConfTag)-tpl.wxs" />
    <Copy SourceFiles="@(WixSoftware)" DestinationFiles="@(WixSoftware->'Wix\%(RecursiveDir)%(Filename)%(Extension)')" />
    <Exec Command="candle -nologo Features.wxs" WorkingDirectory="Wix" />
    <Exec Command="candle -nologo Install$(ConfTag)$(Edition).wxs " WorkingDirectory="Wix" />
    <Exec Command="candle -nologo UserInterface.wxs" WorkingDirectory="Wix" />
    <Exec Command="candle -nologo Files.wxs -sw1044" WorkingDirectory="Wix" />
    <Exec Command="light -nologo wixca.wixlib Features.wixobj Install$(ConfTag)$(Edition).wixobj Files.wixobj UserInterface.wixobj -out Setup$(ConfTag)$(Edition)$(Status)-$(BUILD_NUMBER).msi" WorkingDirectory="Wix" />
