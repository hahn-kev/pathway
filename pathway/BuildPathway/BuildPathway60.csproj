  <Target Name="Build">
    <CallTarget Targets="BeforeBuild" />
    <RemoveDir Directories="Wix" />
    <Message Text="BUILD_NUMBER: $(BUILD_NUMBER)" Importance="high" />
    <UpdateVersion Product="Pathway $(Edition) " Version="1.3.2" FieldworksVersion="$(FwVer)" HelpFile="Pathway_Configuration_Tool_$(Edition).chm" Template="$(ProjectDir)Install$(ConfTag)$(Edition)-tpl.wxs" />
    <UpdateReadme Readme="$(ProjectDir)ReadMePw.rtf" License="$(ProjectDir)License.rtf" Template="$(ProjectDir)UserInterface-tpl.wxs" />
    <ZeroCheck />
    <FileComponents FilesTemplate="Files$(ConfTag)-tpl.wxs" FeaturesTemplate="Features$(ConfTag)-tpl.wxs" />
    <Copy SourceFiles="@(WixSoftware)" DestinationFiles="@(WixSoftware->'Wix\%(RecursiveDir)%(Filename)%(Extension)')" />
    <Exec Command="candle -nologo Features.wxs" WorkingDirectory="Wix" />
    <Exec Command="candle -nologo Install$(ConfTag)$(Edition).wxs " WorkingDirectory="Wix" />
    <Exec Command="candle -nologo UserInterface.wxs" WorkingDirectory="Wix" />
    <Exec Command="candle -nologo Files.wxs -sw1044" WorkingDirectory="Wix" />
    <Exec Command="light -nologo wixca.wixlib Features.wixobj Install$(ConfTag)$(Edition).wixobj Files.wixobj UserInterface.wixobj -out Setup$(ConfTag)$(Edition)$(FwVer)$(Status)-$(BUILD_NUMBER).msi" WorkingDirectory="Wix" />
    <Copy SourceFiles=".\Wix\Setup$(ConfTag)$(Edition)$(FwVer)$(Status)-$(BUILD_NUMBER).msi" DestinationFolder="..\Sprint"/>
  </Target>
