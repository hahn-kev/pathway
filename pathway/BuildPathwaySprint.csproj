  <Target Name="TlGetInstaller">
    <MSBuild.ExtensionPack.FileSystem.Folder TaskAction="DeleteAll" Path="XeLaTexConvert" Match="TexLive"/>
    <MakeDir Directories="XeLaTexConvert\TexLive" />
    <CallTarget Targets="TlGetWinInstaller" Condition="$(OS) != 'Linux'"/>
    <CallTarget Targets="TlGetUnxInstaller" Condition="$(OS) == 'Linux'"/>
  </Target>
  <ItemGroup>
    <repo Include="http://ftp.math.purdue.edu/mirrors/ctan.org/systems/texlive/tlnet"/>
    <!-- repo Include="http://mirror.math.ku.edu/tex-archive/systems/texlive/tlnet"/>
    <repo Include="http://mirror.ctan.org/systems/texlive/tlnet"/ -->
  </ItemGroup>
  <Target Name="TlGetWinInstaller">
    <MSBuild.ExtensionPack.FileSystem.Folder TaskAction="Get" Path="XeLaTexConvert" Match="TexLive">
      <Output TaskParameter="Folders" ItemName="InstallerDir"/>
    </MSBuild.ExtensionPack.FileSystem.Folder>
    <MSBuild.ExtensionPack.Framework.TextString TaskAction="Replace" OldString="@InstallerDir@/install-tl.zip" OldValue="@InstallerDir@" NewValue="%(InstallerDir.Identity)">
      <Output PropertyName="RepoZip" TaskParameter="NewString"/>
    </MSBuild.ExtensionPack.Framework.TextString>
    <Message Text="cmd: $(cmd2)" Importance="high" />
    <Message Text="InstallerDir: %(InstallerDir.Identity)" Importance="high" />

    <MSBuild.ExtensionPack.Web.WebClient TaskAction="DownloadFile" Url="%(repo.Identity)/install-tl.zip" FileName="$(RepoZip)"/>
    <Exec Command="dir %(InstallerDir.Identity)"/>
    <MSBuild.ExtensionPack.Compression.Zip TaskAction="Extract" ExtractPath="%(InstallerDir.Identity)" ZipFileName="%(InstallerDir.Identity)\install-tl.zip"/>
    <Exec Command="dir %(InstallerDir.Identity)\*.bat /s"/>
  </Target>
  <Target Name="TlGetUnxInstaller">
    <!--MSBuild.ExtensionPack.FileSystem.Folder TaskAction="DeleteAll" Path="/usr/local" Match="texlive"/ -->
    <!-- MSBuild.ExtensionPack.FileSystem.Folder TaskAction="DeleteAll" Path="~" Match=".texlive2011"/ -->
    <MSBuild.ExtensionPack.FileSystem.Folder TaskAction="Get" Path="XeLaTexConvert" Match="TexLive">
      <Output TaskParameter="Folders" ItemName="InstallerWorkDir"/>
    </MSBuild.ExtensionPack.FileSystem.Folder>
    <Message Text="InstallerWorkDir: %(InstallerWorkDir.Identity)" Importance="high" />
    <MSBuild.ExtensionPack.Framework.TextString TaskAction="Replace" OldString="@InstallerWorkDir@/install-tl-unx.tar.gz" OldValue="@InstallerWorkDir@" NewValue="%(InstallerWorkDir.Identity)">
      <Output PropertyName="ArchiveName" TaskParameter="NewString"/>
    </MSBuild.ExtensionPack.Framework.TextString>
    <Message Text="ArchiveName: $(ArchiveName)" Importance="high" />
    <MSBuild.ExtensionPack.Web.WebClient TaskAction="DownloadFile" Url="http://ftp.math.purdue.edu/mirrors/ctan.org/systems/texlive/tlnet/install-tl-unx.tar.gz" FileName="$(ArchiveName)"/>
    <Exec Command="tar -xzf install-tl-unx.tar.gz" WorkingDirectory="%(InstallerWorkDir.Identity)"/>
  </Target>

