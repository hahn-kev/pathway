    <entry id="apu">
        <lexical-unit>
            <form lang="und-Latn">
                <text>apu</text>
            </form>
        </lexical-unit>
        <sense id="apu_">
            <grammatical-info value="n"/>
            <gloss lang="en">
                <text>lime</text>
            </gloss>
            <reversal>
                <form lang="en">
                    <text>lime</text>
                </form>
            </reversal>
            <definition>
                <form lang="en">
                    <text> lime slaked from burning seashells and used as an ingredient in chewing
                        betelnut </text>
                </form>
            </definition>
        </sense>
    </entry>
    <entry id="opon">
