        <trait name="morph-type" value="root"/>
        <pronunciation>
            <form lang="bzh-fonipa">
                <text>ˈa.ᵐba</text>
            </form>
        </pronunciation>
        <sense id="core_76a3b7cf-fe7c-402f-aeae-2f5178d28a00">
            <grammatical-info value="Noun"/>
            <gloss lang="en">
                <text>core</text>
            </gloss>
            <definition>
                <form lang="en">
                    <text>cob, core, the central section of a cob of corn or of pandanus fruit after
                        the outer seeds or kernels are removed.</text>
                </form>
                <form lang="tpi">
                    <text>bun bilong kon o marita.</text>
                </form>
            </definition>
            <example>
                <form lang="bzh">
                    <text>Aba neggëp loḳ sakom ayo peggo.</text>
                </form>
                <translation type="Free translation">
                    <form lang="en">
                        <text>There is an inner core inside a cob of corn.</text>
                    </form>
                </translation>
            </example>
            <example>
                <form lang="bzh">
                    <text>Desoh taġee gedetë aba ya.</text>
                </form>
                <translation type="Free translation">
                    <form lang="en">
                        <text>They removed the fruit of the pandanus and threw the inner core
                            away.</text>
                    </form>
                </translation>
            </example>
            <trait name="semantic-domain-ddp4" value="1.5.5 Parts of a plant"/>
            <trait name="semantic-domain-ddp4" value="5.2.3.1.2 Food from fruit"/>
            <trait name="anthro-code" value="824"/>
            <reversal type="en">
                <form lang="en">
                    <text>core</text>
                </form>
            </reversal>
        </sense>
    </entry>
