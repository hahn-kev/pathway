    <entry dateCreated="1997-05-09T19:54:54Z" dateModified="2009-02-07T19:17:05Z"
        guid="2655d2d4-9f11-4a2e-9c04-948a9bf75807" id="a-_2655d2d4-9f11-4a2e-9c04-948a9bf75807">
        <lexical-unit>
            <form lang="bzh">
                <text>aa</text>
            </form>
        </lexical-unit>
        <trait name="morph-type" value="prefix"/>
        <pronunciation>
            <form lang="bzh-fonipa">
                <text>a</text>
            </form>
        </pronunciation>
        <sense id="1pMKR_0bd07b9b-ed36-4f58-8c01-cdc65ce180a4">
            <grammatical-info value="Verb">
                <trait name="type" value="inflAffix"/>
                <trait name="Verb-slot" value="Subject Person"/>
            </grammatical-info>
            <gloss lang="en">
                <text>1pMKR</text>
            </gloss>
            <definition>
                <form lang="en">
                    <text>First person plural marker prefixed to verbs stressed on the first
                        syllable of the root in first person dual, trial and plural forms. It is
                        often not marked on the plural exclusive unless something else intervenes
                        between the pronoun <span lang="bzh">he</span> and the verb.</text>
                </form>
            </definition>
            <example>
                <form lang="bzh">
                    <text>He su ayoh vu bë adah vun in ham rë.</text>
                </form>
                <translation type="Free translation">
                    <form lang="en">
                        <text>We are not able to hide it from you.</text>
                    </form>
                </translation>
            </example>
            <example>
                <form lang="bzh">
                    <text>Hil ana sepa ataġ vaha.</text>
                </form>
                <translation type="Free translation">
                    <form lang="en">
                        <text>Let's go and follow my mother's tracks.</text>
                    </form>
                </translation>
            </example>
            <example>
                <form lang="bzh">
                    <text>Alu aluḳ dub loḳ ayed sënë.</text>
                </form>
                <translation type="Free translation">
                    <form lang="en">
                        <text>We two went to school in our own language.</text>
                    </form>
                </translation>
            </example>
            <reversal type="en">
                <form lang="en">
                    <text>First Person Plural Marker</text>
                </form>
            </reversal>
        </sense>
    </entry>
